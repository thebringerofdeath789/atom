<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorBlack"
    android:paddingLeft="@dimen/dp_16"
    android:paddingTop="@dimen/dp_10"
    android:paddingBottom="@dimen/dp_16"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="@dimen/sp_12"
        android:textColor="@color/colorText"
        android:id="@+id/tv_app_tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sys_version_app"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textSize="@dimen/sp_12"
        android:textColor="@color/colorText"
        android:gravity="center_vertical"
        android:id="@+id/tv_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" NA"
        app:layout_constraintLeft_toRightOf="@+id/tv_app_tips"
        app:layout_constraintTop_toTopOf="@+id/tv_app_tips"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_flight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_app_tips">
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/layout_flight_update"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/tv_flight"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <View
                android:background="@drawable/dash_line"
                android:visibility="visible"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:layout_weight="1"
                android:layerType="software"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_marginEnd="@dimen/dp_10"/>
            <TextView
                android:id="@+id/tv_flight_update"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/update"/>
        </LinearLayout>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="@color/colorText"
            android:id="@+id/tv_flight_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sys_version_flight"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="@color/colorText"
            android:gravity="center_vertical"
            android:id="@+id/tv_flight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  NA"
            app:layout_constraintLeft_toRightOf="@+id/tv_flight_tips"
            app:layout_constraintTop_toTopOf="@+id/tv_flight_tips"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
