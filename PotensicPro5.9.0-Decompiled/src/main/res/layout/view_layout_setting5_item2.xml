            android:id="@+id/tv_fpv"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  NA"
            app:layout_constraintLeft_toRightOf="@+id/tv_fpv_tips"
