<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/view_layout_gimbal_fine_tuning_0"
    android:background="@color/black"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.logan.opengl.JfGLSurfaceView
        android:id="@+id/view_video"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <TextView
        android:textSize="75sp"
        android:textStyle="bold"
        android:id="@+id/tv_count_down"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/font_white_15"/>
    <View
        android:background="@color/colorFiftyPercent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <View
        android:background="@drawable/bg_line_gimbal_fine"
        android:layout_width="2dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:background="@drawable/bg_line_gimbal_fine"
        android:layout_width="0dp"
        android:layout_height="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageButton
        android:id="@+id/btn_close"
        android:background="@null"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:src="@mipmap/icon_btn_cancel_download"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ScrollView
        android:scrollbars="none"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="0.728">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:background="@drawable/bg_gimbal_fine_tuning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp">
            <TextView
                android:textSize="14sp"
                android:textColor="@color/white"
                android:id="@+id/tv_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/gimbal_level"
                app:layout_constraintBottom_toBottomOf="@+id/ll_level"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/ll_level"
                app:layout_constraintTop_toTopOf="@+id/ll_level"/>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/ll_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_10"
                app:layout_constraintLeft_toRightOf="@+id/tv_level"
                app:layout_constraintRight_toLeftOf="@+id/tv_default_value_level"
                app:layout_constraintTop_toTopOf="parent">
                <ImageButton
                    android:id="@+id/btn_reduce_level"
                    android:tag="binding_1"
                    android:background="@null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_reduce_nor"/>
                <com.ipotensic.kernel.view.CursorStringEditText
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@+id/et_level_value"
                    android:tag="binding_2"
                    android:background="@mipmap/button_ghost_import_nor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gimbal_edit_text_0_value"
                    android:maxLength="4"
                    android:inputType="numberSigned"
                    android:imeOptions="flagNoExtractUi|flagNoFullscreen|actionDone"/>
                <ImageButton
                    android:id="@+id/btn_plus_level"
                    android:tag="binding_3"
                    android:background="@null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_plus_nor"/>
            </LinearLayout>
            <TextView
                android:textColor="@color/color_media_text"
                android:gravity="center"
                android:id="@+id/tv_default_value_level"
                android:tag="binding_4"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/gimbal_default_value"
                app:layout_constraintBottom_toBottomOf="@+id/ll_level"
                app:layout_constraintLeft_toRightOf="@+id/ll_level"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/ll_level"/>
            <View
                android:id="@+id/view_line"
                android:background="@mipmap/img_gimbal_calibration_dashed_line"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                app:layout_constraintLeft_toLeftOf="@+id/tv_level"
                app:layout_constraintRight_toRightOf="@+id/tv_default_value_level"
                app:layout_constraintTop_toBottomOf="@+id/ll_level"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/white"
                android:id="@+id/tv_yaw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/gimbal_yaw"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/view_line"
                app:layout_constraintTop_toBottomOf="@+id/view_line"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/ll_level"
                app:layout_constraintRight_toRightOf="@+id/ll_level"
                app:layout_constraintTop_toTopOf="@+id/view_line">
                <ImageButton
                    android:id="@+id/btn_reduce_yaw"
                    android:tag="binding_5"
                    android:background="@null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_reduce_nor"/>
                <com.ipotensic.kernel.view.CursorStringEditText
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@+id/et_yaw_value"
                    android:tag="binding_6"
                    android:background="@mipmap/button_ghost_import_nor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gimbal_edit_text_0_value"
                    android:maxLength="4"
                    android:inputType="numberSigned"
                    android:imeOptions="flagNoExtractUi|flagNoFullscreen|actionDone"/>
                <ImageButton
                    android:id="@+id/btn_plus_yaw"
                    android:tag="binding_7"
                    android:background="@null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_plus_nor"/>
            </LinearLayout>
            <TextView
                android:textColor="@color/color_media_text"
                android:gravity="center"
                android:id="@+id/tv_default_value_yaw"
                android:tag="binding_8"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/gimbal_default_value"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="@+id/view_line"
                app:layout_constraintTop_toTopOf="@+id/view_line"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
