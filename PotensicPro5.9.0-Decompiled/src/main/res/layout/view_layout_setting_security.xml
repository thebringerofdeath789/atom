<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:tag="layout/view_layout_setting_security_0"
    android:background="@color/black"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.ipotensic.kernel.view.CommonTitleView
        android:id="@+id/view_title"
        android:background="@drawable/bg_setting_calibration_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tvTitle="@string/txt_flight_security"/>
    <ScrollView
        android:orientation="vertical"
        android:id="@+id/layout_setting"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_48">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/base_line_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_begin="@dimen/dp_16"/>
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/base_line_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_end="@dimen/dp_24"/>
            <com.ipotensic.kernel.view.CommonSwitchView
                android:id="@+id/csv_beginner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:tvTitle="@string/sys_basic_beginner_mode"/>
            <View
                android:id="@+id/bg_unit"
                android:background="@drawable/bg_setting_calibration_2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintBottom_toBottomOf="@+id/space_unit"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/csv_beginner"/>
            <TextView
                android:id="@+id/tv_unit"
                android:paddingTop="@dimen/dp_flight_setting_padding_left_right"
                android:paddingBottom="@dimen/dp_flight_setting_padding_left_right"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/sys_units"
                android:layout_marginStart="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toLeftOf="@+id/bg_unit"
                app:layout_constraintTop_toTopOf="@+id/bg_unit"
                style="@style/font_white_15"/>
            <com.ipotensic.kernel.view.CommonSelectionView
                android:id="@+id/csv_unit"
                android:layout_width="181dp"
                android:layout_height="@dimen/dp_32"
                android:layout_marginEnd="@dimen/dp_24"
                app:childText1="@string/sys_units_metric"
                app:childText2="@string/sys_units_imperial"
                app:layout_constraintBottom_toBottomOf="@+id/tv_unit"
                app:layout_constraintRight_toRightOf="@+id/bg_unit"
                app:layout_constraintTop_toTopOf="@+id/tv_unit"
                app:layout_constraintVertical_bias="0.333"/>
            <View
                android:id="@+id/line1"
                android:background="@color/colorGray"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="6dp"
                app:layout_constraintEnd_toEndOf="@+id/csv_unit"
                app:layout_constraintStart_toStartOf="@+id/tv_unit"
                app:layout_constraintTop_toBottomOf="@+id/tv_unit"/>
            <TextView
                android:id="@+id/tv_speed_setting"
                android:paddingTop="@dimen/dp_flight_setting_padding_left_right"
                android:paddingBottom="@dimen/dp_flight_setting_padding_left_right"
                android:layout_marginTop="4dp"
                android:text="@string/sys_basic_gear_setting"
                app:layout_constraintBottom_toBottomOf="@+id/bg_unit"
                app:layout_constraintLeft_toLeftOf="@+id/tv_unit"
                app:layout_constraintTop_toBottomOf="@+id/line1"
                style="@style/font_white_15"/>
            <View
                android:id="@+id/space_unit"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_10"
                app:layout_constraintStart_toStartOf="@+id/bg_unit"
                app:layout_constraintTop_toBottomOf="@+id/tv_speed_setting"/>
            <com.ipotensic.kernel.view.CommonSelectionView
                android:id="@+id/crv"
                android:layout_width="272dp"
                android:layout_height="@dimen/dp_32"
                android:layout_marginEnd="@dimen/dp_24"
                app:layout_constraintBottom_toBottomOf="@+id/tv_speed_setting"
                app:layout_constraintRight_toRightOf="@+id/bg_unit"
                app:layout_constraintTop_toTopOf="@+id/tv_speed_setting"/>
            <View
                android:background="@drawable/bg_setting_calibration_2"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_0"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bg_unit"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutFence"
                android:background="@drawable/bg_setting_calibration_2"
                android:paddingLeft="@dimen/dp_flight_setting_padding_left_right"
                android:paddingTop="10dp"
                android:paddingRight="@dimen/dp_24"
                android:paddingBottom="10dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bg_unit">
                <TextView
                    android:id="@+id/tvSignalLost"
                    android:paddingTop="@dimen/dp_flight_setting_padding_left_right"
                    android:paddingBottom="@dimen/dp_flight_setting_padding_left_right"
                    android:text="@string/txt_flight_lost"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/font_white_15"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:id="@+id/tv_lost_action_warning"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/dp_3"
                    android:text="@string/txt_lost_action_warning"
                    android:drawablePadding="@dimen/dp_2"
                    android:drawableStart="@mipmap/img_icon_careful"
                    app:layout_constraintLeft_toLeftOf="@+id/tvSignalLost"
                    app:layout_constraintTop_toBottomOf="@+id/tvSignalLost"
                    style="@style/font_white_11"/>
                <ImageButton
                    android:id="@+id/tvLostChildFunction"
                    android:background="@null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintLeft_toLeftOf="@+id/tvSignalLost"
                    app:layout_constraintTop_toBottomOf="@+id/tv_lost_action_warning"/>
                <TextView
                    android:id="@+id/tv_lost_child"
                    android:tag="binding_1"
                    android:padding="@dimen/dp_5"
                    android:text="@string/txt_lost_to_return"
                    android:drawablePadding="@dimen/dp_5"
                    android:drawableStart="@mipmap/img_btn_signal_lost_child_return_unselect"
                    app:layout_constraintStart_toStartOf="@+id/tvLostChildFunction"
                    app:layout_constraintTop_toBottomOf="@+id/tvLostChildFunction"
                    style="@style/font_white_13"/>
                <com.ipotensic.kernel.view.CommonSelectionView
                    android:id="@+id/csv_lost"
                    android:layout_width="272dp"
                    android:layout_height="@dimen/dp_32"
                    app:childText1="@string/txt_return"
                    app:childText2="@string/txt_land"
                    app:childText3="@string/txt_hover"
                    app:layout_constraintBottom_toBottomOf="@+id/tvSignalLost"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/tvSignalLost"/>
                <View
                    android:id="@+id/line2"
                    android:background="@color/colorGray"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/tv_lost_child"/>
                <TextView
                    android:id="@+id/tv_return_height"
                    android:tag="binding_2"
                    android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/line2"
                    style="@style/font_white_15"/>
                <ImageView
                    android:id="@+id/iv_play_return_height_video"
                    android:padding="@dimen/dp_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_play_return_height"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_return_height"
                    app:layout_constraintStart_toEndOf="@+id/tv_return_height"
                    app:layout_constraintTop_toTopOf="@+id/tv_return_height"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/layoutReturnHeight"
                    android:tag="binding_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_return_height">
                    <com.ipotensic.kernel.view.CursorEditText
                        android:textSize="@dimen/txt_size_setting_big"
                        android:textColor="@color/text_color_setting_blue"
                        android:gravity="center"
                        android:id="@+id/edt_return_height"
                        android:tag="binding_4"
                        android:background="@mipmap/img_bg_white_input_stroke"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:cursorVisible="true"
                        android:numeric="integer"
                        android:layout_weight="1"
                        android:imeOptions="flagNoExtractUi|flagNoFullscreen|actionDone"/>
                    <TextView
                        android:tag="binding_5"
                        android:paddingLeft="3dp"
                        style="@style/font_blue_13"/>
                    <com.ipotensic.kernel.view.CustomSeekbar
                        android:tag="binding_6"
                        android:layout_width="@dimen/dp_0"
                        android:layout_height="wrap_content"
                        android:maxHeight="@dimen/dp_4"
                        android:max="40"
                        android:progressDrawable="@drawable/seekbar_style_blue"
                        android:minHeight="@dimen/dp_4"
                        android:thumb="@drawable/selector_thumb_bar"
                        android:thumbOffset="@dimen/dp_0"
                        android:layout_weight="6"
                        android:paddingStart="@dimen/dp_5"
                        android:paddingEnd="@dimen/dp_0"
                        android:splitTrack="false"/>
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1.5"/>
                </LinearLayout>
                <View
                    android:id="@+id/line3"
                    android:background="@color/colorGray"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/layoutReturnHeight"/>
                <com.ipotensic.kernel.view.CommonSwitchView
                    android:id="@+id/view_silent_return"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/line3"
                    app:switchButtonPaddingRight="@dimen/dp_0"
                    app:tvContent="@string/settings_safety_silent_return_tips"
                    app:tvTitle="@string/settings_safety_silent_return_title"
                    app:tvTitlePaddingLeft="@dimen/dp_0"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintlayoutFlightFence"
                android:background="@drawable/bg_setting_calibration_2"
                android:paddingLeft="@dimen/dp_flight_setting_padding_left_right"
                android:paddingTop="10dp"
                android:paddingRight="@dimen/dp_24"
                android:paddingBottom="10dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layoutFence">
                <TextView
                    android:id="@+id/tv_flight_fence"
                    android:tag="binding_7"
                    android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                    android:text="@string/txt_fly_fence"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/font_white_15"/>
                <TextView
                    android:id="@+id/tvHeightLimit"
                    android:tag="binding_8"
                    android:layout_marginTop="10dp"
                    android:text=""
                    app:layout_constraintLeft_toLeftOf="@+id/tv_flight_fence"
                    app:layout_constraintTop_toBottomOf="@+id/tv_flight_fence"
                    style="@style/font_gray_13"/>
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/text_color_setting_red"
                    android:gravity="center"
                    android:id="@+id/tv_altitude_careful_tips"
                    android:tag="binding_9"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/settings_safety_virtual_fence_altitude_limit_over_tip"
                    android:drawableLeft="@mipmap/icon_fly_fence_careful_tips"
                    android:drawablePadding="@dimen/dp_2"
                    app:layout_constraintLeft_toLeftOf="@+id/tvHeightLimit"
                    app:layout_constraintTop_toBottomOf="@+id/tvHeightLimit"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/layoutFlightFence"
                    android:tag="binding_10"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_altitude_careful_tips">
                    <com.ipotensic.kernel.view.CursorEditText
                        android:textSize="@dimen/txt_size_setting_big"
                        android:textColor="@color/text_color_setting_blue"
                        android:gravity="center"
                        android:id="@+id/edtHeightLimit"
                        android:tag="binding_11"
                        android:background="@mipmap/img_bg_white_input_stroke"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:cursorVisible="true"
                        android:numeric="integer"
                        android:layout_weight="1"
                        android:imeOptions="flagNoExtractUi|flagNoFullscreen|actionDone"/>
                    <TextView
                        android:id="@+id/tvUnitHeightLimit"
                        android:tag="binding_12"
                        android:paddingLeft="3dp"
                        style="@style/font_blue_13"/>
                    <com.ipotensic.kernel.view.CustomSeekbar
                        android:id="@+id/seekbarHeightLimit"
                        android:tag="binding_13"
                        android:layout_width="@dimen/dp_0"
                        android:layout_height="wrap_content"
                        android:maxHeight="@dimen/dp_4"
                        android:max="40"
                        android:progressDrawable="@drawable/seekbar_style_blue"
                        android:minHeight="@dimen/dp_4"
                        android:thumb="@drawable/selector_thumb_bar"
                        android:thumbOffset="@dimen/dp_0"
                        android:layout_weight="6"
                        android:paddingStart="@dimen/dp_5"
                        android:paddingEnd="@dimen/dp_0"
                        android:splitTrack="false"/>
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1.5"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/tvDistanceLimit"
                    android:tag="binding_14"
                    android:layout_marginTop="10dp"
                    app:layout_constraintLeft_toLeftOf="@+id/layoutFlightFence"
                    app:layout_constraintTop_toBottomOf="@+id/layoutFlightFence"
                    style="@style/font_gray_13"/>
                <TextView
                    android:id="@+id/tv_height_no_limit"
                    android:text="@string/txt_fly_fence_no_limit"
                    android:layout_marginEnd="@dimen/dp_9"
                    app:layout_constraintBottom_toBottomOf="@+id/switch_btn_no_limit"
                    app:layout_constraintEnd_toStartOf="@+id/switch_btn_no_limit"
                    app:layout_constraintTop_toTopOf="@+id/switch_btn_no_limit"
                    style="@style/font_gray_11"/>
                <com.ipotensic.kernel.view.SwitchButton
                    android:id="@+id/switch_btn_no_limit"
                    android:tag="binding_15"
                    android:layout_width="50dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="@+id/tvDistanceLimit"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/tvDistanceLimit"/>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/layoutDistanceLimit"
                    android:tag="binding_16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvDistanceLimit">
                    <com.ipotensic.kernel.view.CursorEditText
                        android:textSize="@dimen/txt_size_setting_big"
                        android:textColor="@color/text_color_setting_blue"
                        android:gravity="center"
                        android:id="@+id/edtDistanceLimit"
                        android:background="@mipmap/img_bg_white_input_stroke"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:cursorVisible="true"
                        android:numeric="integer"
                        android:layout_weight="1"
                        android:imeOptions="flagNoExtractUi|flagNoFullscreen|actionDone"/>
                    <TextView
                        android:id="@+id/tvUnitDistanceLimit"
                        android:tag="binding_17"
                        android:paddingLeft="3dp"
                        style="@style/font_blue_13"/>
                    <com.ipotensic.kernel.view.CustomSeekbar
                        android:id="@+id/seekbarDistanceLimit"
                        android:layout_width="@dimen/dp_0"
                        android:layout_height="wrap_content"
                        android:maxHeight="@dimen/dp_4"
                        android:max="40"
                        android:progressDrawable="@drawable/seekbar_style_blue"
                        android:minHeight="@dimen/dp_4"
                        android:thumb="@drawable/selector_thumb_bar"
                        android:thumbOffset="@dimen/dp_0"
                        android:layout_weight="6"
                        android:paddingStart="@dimen/dp_5"
                        android:paddingEnd="@dimen/dp_0"
                        android:splitTrack="false"/>
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1.5"/>
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <com.ipotensic.kernel.view.CommonSwitchView
                android:id="@+id/view_fight_tip"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintlayoutFlightFence"
                app:tvContent="@string/txt_flight_safety_and_tips_tips"
                app:tvTitle="@string/txt_flight_safety_and_tips"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/bg_battery"
                android:background="@drawable/bg_setting_calibration_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_flight_setting_padding_left_right"
                android:layout_marginBottom="@dimen/dp_15"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/base_line_left"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view_fight_tip">
                <TextView
                    android:id="@+id/tvBatteryInfo"
                    android:tag="binding_18"
                    android:paddingTop="@dimen/dp_10"
                    android:paddingBottom="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:text="@string/txt_flight_battery_info"
                    android:drawableEnd="@mipmap/img_btn_function_open"
                    android:paddingEnd="@dimen/dp_24"
                    android:layout_marginStart="@dimen/dp_flight_setting_padding_left_right"
                    android:layout_marginVertical="@dimen/dp_10"
                    android:paddingVertical="@dimen/dp_10"
                    style="@style/font_white_15"/>
                <ViewStub
                    android:id="@+id/vs_battery"
                    android:layout="@layout/view_layout_setting_battery"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_130"
                    android:layout_marginBottom="@dimen/dp_15"/>
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <TextView
        android:textSize="@dimen/sp_12"
        android:textColor="@color/text_mini_geo_color"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:id="@+id/tv_speed_change"
        android:background="@drawable/bg_left_error_tips"
        android:paddingLeft="@dimen/dp_16"
        android:paddingTop="@dimen/dp_12"
        android:paddingRight="@dimen/dp_16"
        android:paddingBottom="@dimen/dp_12"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_100"
        android:layout_marginRight="@dimen/dp_100"
        android:text="@string/settings_safety_speed_settings_sport_mode_caution_tips"
        android:layout_marginHorizontal="@dimen/dp_100"
        android:paddingHorizontal="@dimen/dp_16"
        android:paddingVertical="@dimen/dp_12"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_flight_tips"/>
</FrameLayout>
