<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorSixtyPercent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/guide_scrollView"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/ll_camera_set"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/iv_set"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_38"
                android:src="@mipmap/icon_camera_set_btn"
                android:scaleType="center"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <View
                android:id="@+id/line1"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_set"
                style="@style/view_line"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_1"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_set">
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center_vertical"
                    android:id="@+id/tv_code_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_grid"
                    android:drawableLeft="@mipmap/icon_grid"
                    android:drawablePadding="@dimen/dp_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <com.ipotensic.kernel.view.SwitchButton
                    android:id="@+id/slideBtn"
                    android:clickable="false"
                    android:layout_width="@dimen/dp_50"
                    android:layout_height="@dimen/dp_25"
                    android:layout_marginEnd="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/line2"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_1"
                style="@style/view_line"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_2"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line2">
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_ev"
                    android:drawableLeft="@mipmap/icon_ev"
                    android:drawablePadding="@dimen/dp_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_btn_function_open"
                    android:paddingEnd="@dimen/dp_29"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/line3"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_2"
                style="@style/view_line"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_3"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line3">
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_video_segmentation"
                    android:drawableLeft="@mipmap/icon_video_segmentation"
                    android:drawablePadding="@dimen/dp_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_btn_function_open"
                    android:paddingEnd="@dimen/dp_29"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/line4"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_3"
                style="@style/view_line"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_4"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line4">
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_photo_quality"
                    android:drawableLeft="@mipmap/icon_resolution_ratio"
                    android:drawablePadding="@dimen/dp_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_btn_function_open"
                    android:paddingEnd="@dimen/dp_29"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/line5"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_4"
                style="@style/view_line"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_5"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line5">
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_resolution_ratio"
                    android:drawableLeft="@mipmap/icon_resolution_ratio"
                    android:drawablePadding="@dimen/dp_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_btn_function_open"
                    android:paddingEnd="@dimen/dp_29"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/line6"
                android:layout_marginEnd="@dimen/dp_0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_5"
                style="@style/view_line"/>
            <View
                android:id="@+id/view_bg"
                android:background="@color/colorSixtyPercent"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_0"
                app:layout_constraintBottom_toBottomOf="@+id/line6"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/iv_set"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_format_sd"
                android:paddingLeft="@dimen/dp_12"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line6">
                <ImageView
                    android:id="@+id/tv_sd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_sdcard"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/colorWhite"
                    android:gravity="center"
                    android:id="@+id/tv_sd_format"
                    android:background="@drawable/bg_format_sd"
                    android:padding="@dimen/dp_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/camera_sd_format"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.55"/>
                <ImageView
                    android:id="@+id/iv_guide_cycle_2"
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bg_guide_sd_cycle_btn"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <ImageView
                android:id="@+id/iv_rect_format"
                android:background="@drawable/bg_guide_rect_btn"
                android:visibility="invisible"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/cl_format_sd"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/line6"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
