<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/view_layout_compass_calibration_0"
    android:background="@drawable/bg_color_black"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:background="@color/colorFiftyPercent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <TextView
        android:textSize="20sp"
        android:textColor="@color/white"
        android:id="@+id/tv_compass_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:text="@string/setting_compass_calibration"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageButton
        android:id="@+id/btn_close"
        android:background="@null"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:src="@mipmap/icon_btn_cancel_download"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_compass_calibration"
        android:tag="binding_1"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_close">
        <ImageView
            android:id="@+id/pic_compass_calibration"
            android:tag="binding_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginRight="@dimen/dp_48"
            android:layout_marginBottom="@dimen/dp_8"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toTopOf="@+id/btn_start"
            app:layout_constraintDimensionRatio="4:3"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0"/>
        <TextView
            android:textSize="12sp"
            android:textColor="@color/white"
            android:id="@+id/tv_compass_height_tip"
            android:tag="binding_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_157"
            android:layout_marginBottom="@dimen/dp_42"
            app:layout_constraintBottom_toBottomOf="@+id/pic_compass_calibration"
            app:layout_constraintRight_toRightOf="@+id/pic_compass_calibration"/>
        <TextView
            android:textSize="12sp"
            android:textColor="@color/white"
            android:tag="binding_4"
            android:background="@drawable/bg_round_rectangle"
            android:padding="@dimen/dp_10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_71"
            android:layout_marginRight="@dimen/dp_25"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/pic_compass_calibration"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.3"/>
        <Button
            android:textSize="16sp"
            android:textColor="@color/white"
            android:id="@+id/btn_start"
            android:tag="binding_5"
            android:background="@drawable/bg_compass_calibration_start_button"
            android:paddingLeft="@dimen/dp_40"
            android:paddingRight="40dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_35"
            android:text="@string/compass_start_calibration"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_calibration_start"
        android:tag="binding_6"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_close">
        <ImageView
            android:id="@+id/iv_rotating_axis"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/dp_6"
            android:src="@mipmap/img_bg_drone_calibration"
            android:scaleType="fitCenter"
            app:layout_constraintDimensionRatio="31:105"
            app:layout_constraintHeight_percent="0.65"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/img_atom"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="@dimen/dp_15"
            android:src="@drawable/anim_geo_horizontal_atom"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="@+id/iv_rotating_axis"
            app:layout_constraintDimensionRatio="96:72"
            app:layout_constraintHeight_percent="0.51"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
        <TextView
            android:textSize="12sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/tv_mini_geo_step_tips"
            android:background="@drawable/bg_mini_geo_tip"
            android:paddingLeft="22dp"
            android:paddingRight="5dp"
            android:layout_width="260dp"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/dp_24"
            android:layout_marginBottom="@dimen/dp_8"
            android:text="@string/dialog_geo_horizontal_tips"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/img_atom"/>
        <ImageView
            android:id="@+id/iv_mini_geo_step_tips"
            android:tag="binding_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_mini_geo_step_tips"
            app:layout_constraintLeft_toLeftOf="@+id/tv_mini_geo_step_tips"
            app:layout_constraintTop_toTopOf="@+id/tv_mini_geo_step_tips"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_calibration_result"
        android:tag="binding_8"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/iv_calibration_result"
            android:tag="binding_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0.2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/tv_fail_notice"
            android:paddingLeft="@dimen/dp_16"
            android:paddingRight="@dimen/dp_16"
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp_16"
            android:text="@string/settings_calibration_compass_calibration_failed_tips_magnetic_interference_detected"
            android:paddingHorizontal="@dimen/dp_16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_calibration_result"
            style="@style/font_white_15"/>
        <TextView
            android:id="@+id/tv_count_down"
            android:layout_marginTop="10dp"
            android:text="@string/exiting"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_fail_notice"
            style="@style/font_gray_13"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
