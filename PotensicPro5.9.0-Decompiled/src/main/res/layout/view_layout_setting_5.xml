<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorOpaque"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textSize="@dimen/sp_14"
        android:textColor="@color/colorWhite"
        android:id="@+id/tv_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_20"
        android:text="@string/sys_general_set"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/iv_back"
        android:padding="@dimen/dp_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_bg_setting_back"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ScrollView
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0"
        android:layout_marginTop="@dimen/dp_25"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_setting">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include
                android:id="@+id/include3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                layout="@layout/view_layout_setting5_item3"/>
            <View style="@style/view_line"/>
            <include
                android:id="@+id/include1"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                layout="@layout/view_layout_setting5_item1"/>
            <View style="@style/view_line"/>
            <com.ipotensic.kernel.view.ExpandableView
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:right_icon="@mipmap/img_btn_function_open"
                app:title="@string/sys_general_about"
                app:titleColor="@color/colorWhite"
                app:titleSizeSp="14"
                app:topViewPaddingBottom="@dimen/dp_16"
                app:topViewPaddingLeft="@dimen/dp_16"
                app:topViewPaddingRight="@dimen/dp_40"
                app:topViewPaddingTop="@dimen/dp_16">
                <include
                    android:id="@+id/include2"
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/view_layout_setting5_item2"/>
            </com.ipotensic.kernel.view.ExpandableView>
            <View style="@style/view_line"/>
            <RelativeLayout
                android:paddingLeft="@dimen/dp_16"
                android:paddingTop="@dimen/dp_16"
                android:paddingRight="@dimen/dp_20"
                android:paddingBottom="@dimen/dp_16"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/colorWhite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目区分"/>
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_cur_project"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/dp_20"
                    android:text="@string/test_data"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
            <View
                android:id="@+id/line_sn"
                android:visibility="gone"
                style="@style/view_line"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_sn"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_flight_type"
                    android:paddingLeft="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_16"
                    android:paddingRight="@dimen/dp_20"
                    android:paddingBottom="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="飞机型号："/>
                <View style="@style/view_line"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_flight_sn"
                    android:paddingLeft="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_16"
                    android:paddingRight="@dimen/dp_20"
                    android:paddingBottom="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="飞控SN码："/>
                <View style="@style/view_line"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_remote_sn"
                    android:paddingLeft="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_16"
                    android:paddingRight="@dimen/dp_20"
                    android:paddingBottom="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="遥控器SN码："/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
